  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "eject": "craco eject",
    "test": "jest",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "prepare": "husky install",
    "cypress:open": "cypress open"
